{
  "spacebro": {
    "host": "127.0.0.1",
    "port": 8888,
    "channelName": "dmx-bro",
    "client": {
      "name": "dmx-bro",
      "description": "A spacebro client forwarding dmx signals"
    }
  },
  "dmx": {
    "universes": {
      "default": {
        "output": {
          "driver": "enttec-usb-dmx-pro",
